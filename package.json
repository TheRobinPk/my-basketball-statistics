{
  "name": "basketball-shoot-around-app",
  "version": "1.0.0",
  "scripts": {
    "start": "ENVIRONMENT=develop expo start --dev-client",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "test": "jest --forceExit",
    "build": "npm run lint && npm run test",
    "build:android:develop": "npm run build && eas build --platform android --profile development --non-interactive",
    "build:android:preview": "npm run build && eas build --platform android --profile preview --non-interactive",
    "build:android:production": "npm run build && eas build --platform android --profile production --non-interactive",
    "typeorm": "typeorm-ts-node-commonjs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7",
    "react-native-safe-area-context": "4.2.4",
    "react-native-reanimated": "~2.8.0",
    "reanimated-collapsible-helpers": "^1.0.0",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-paper": "^5.0.0-rc.3",
    "react-native-calendars": "^1.1287.0",
    "@react-native-community/datetimepicker": "^6.1.2",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.1",
    "expo-file-system": "~14.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-system-ui": "~1.2.0",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "^0.13.3",
    "expo-localization": "~13.0.0",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/drawer": "^6.4.3",
    "redux": "^4.2.0",
    "@reduxjs/toolkit": "^1.8.3",
    "react-redux": "^8.0.2",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "moment": "^2.29.4",
    "i18n-js": "^4.0.2",
    "metro-minify-terser": "^0.71.3",
    "patch-package": "^6.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^28.1.5",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-native-datepicker": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@types/randomcolor": "^0.5.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-native": "^3.10.0",
    "jest": "^28.1.3",
    "jest-create-mock-instance": "^2.0.0",
    "jest-expo": "^45.0.1",
    "react-native-testing-library": "2.2.0",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "private": true
}
