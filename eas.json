{
  "cli": {
    "version": ">= 0.55.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "releaseChannel": "develop",
      "env": {
        "ENVIRONMENT": "develop"
      }
    },
    "preview": {
      "distribution": "internal",
      "releaseChannel": "preview",
      "env": {
        "ENVIRONMENT": "preview"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "releaseChannel": "production",
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  }
}
